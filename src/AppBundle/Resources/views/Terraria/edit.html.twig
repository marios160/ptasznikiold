{# Edycja terrariow #}
{% extends 'base.html.twig' %}
{% form_theme form 'form/fields.html.twig' %}

{% block title %}Edytuj dane terrarium{% endblock %}

{% block body %}
    <h1>Edytuj dane terrarium</h1>
    <div class="formularz">
        {{ form_start(form) }}
        {{ form_errors(form) }}

        {{ form_row(form.nazwa) }}
        {{ form_row(form.opis) }}
        {{ form_row(form.pojemnosc) }}
        {{ form_row(form.rozmiar) }}
        {{ form_row(form.zapisz) }}
        {{ form_end(form) }}
    </div>
{% endblock %}